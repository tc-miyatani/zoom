<% content_for :head do %>
<%= stylesheet_link_tag asset_path('/assets/pages/test.css') %>
<% end %>

<%= form_with model: @room, local: true do |f| %>
  <%= f.text_field :title, null: false, placeholder:"部屋名" %>
  <%= f.text_field :name, null: false, placeholder:"ユーザー名" %>
  <%= f.submit "部屋を作る" %>
<% end %>

<%= javascript_include_tag asset_path('/assets/test.js') %>

<script>
<%= render partial: 'test.js', locals: {msg: 'hello'} %>
</script>
